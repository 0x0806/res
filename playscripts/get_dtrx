wget https://github.com/a-shvedov/tmp-res/blob/master/src/dtrx.tar.gz
tar -xvf dtrx-7.1.tar.gz
cd dtrx-7.1
python setup.py install --prefix=/usr/local

###opt:

-r, -recursive ;
--one, --one-entry:

1)    inside ,
2)    rename ,
3)    here ,

-o, --overwrite ;
-f, --flat ;
-l , -t , --list , --table ;
-m , --metadata ;
-n, --noninteractive .
